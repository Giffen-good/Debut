{% if video_url != blank %} 
    <video class="psa" height="100%" width="100%" background autoplay muted playsinline controls="false">
        <source src="{{ video_url }}">
    </video>
    {% if image != blank %} 
        {% render 'responsive-image', image:image, custom_classes:'overlay-img psa h100 w100 ofc' %}
    {% endif %}
{% else %}
    {% if image != blank %} 
        {% render 'responsive-image', image:image, custom_classes:'overlay-img psa h100 w100 ofc' %}
    {% endif %}
{% endif %}